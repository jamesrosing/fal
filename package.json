    "db:types": "node --require dotenv/config --loader ts-node/esm scripts/generate-types.ts dotenv_config_path=.env.local",
    "media:migrate": "node scripts/migrate-media.js",
    "media:verify": "node scripts/verify-media.js",
    "media:register": "node scripts/register-media-assets.js",
    "media:structure": ".\\scripts\\create-media-structure.bat",
    "media:align": "node scripts/align-cloudinary-structure.js",
    "media:update-paths": "node scripts/update-component-paths.js",
